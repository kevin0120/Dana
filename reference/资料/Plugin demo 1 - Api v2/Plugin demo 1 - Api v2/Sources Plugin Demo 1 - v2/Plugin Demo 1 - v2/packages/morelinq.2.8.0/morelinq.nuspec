        <dependency id="System.ValueTuple" version="4.4.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Data.DataSetExtensions" targetFramework=".NETFramework4.00.0" />
      <frameworkAssembly assemblyName="System.Data" targetFramework=".NETFramework4.00.0" />
      <frameworkAssembly assemblyName="System" targetFramework=".NETFramework4.00.0" />
      <frameworkAssembly assemblyName="System.Xml" targetFramework=".NETFramework4.00.0" />
    </frameworkAssemblies>
